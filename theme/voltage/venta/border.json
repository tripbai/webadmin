{
    "border-width": {
		"rule": "border-width:{value}px",
		"type": "list",
		"values": ["1","2","3","4","5","6","7","8"]
	},
    "border-radius-extra-small": {
		"rule": "border-radius:{value}px",
		"type": "minmax",
		"values": [1, 24]
	},
    "border-radius-small": {
		"rule": "border-radius:{value}px",
		"type": "minmax",
		"values": [25, 100]
	},
    "border-radius-medium": {
		"rule": "border-radius:{value}px",
		"type": "minmax",
		"values": [100, 300]
	},
	"border-top-style": {
		"rule": "border-top-style:{value}",
		"type": "list",
		"values": ["solid", "dashed", "dotted", "double", "groove", "ridge", "inset", "outset"]
	},
	"border-bottom-style": {
		"rule": "border-bottom-style:{value}",
		"type": "list",
		"values": ["solid", "dashed", "dotted", "double", "groove", "ridge", "inset", "outset"]
	}
}